{
    "defaultLocale": "en_US",
    "inputPath": "./",
    "outputPath": "./output",
    "contextDelimiterKeys": ":",
    "contextDelimiterFiles": "__",
    "nameMatchExp": "[\\w]+",
    "localeRegionExp": "[a-z]{2}_[A-Z]{2}",
    "keyMatchExp": "[a-z_]+",
    "ignoreFiles": [
        ".DS_Store"
    ]
}